@model IEnumerable<IPM.API.Models.Disease_Symptom>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Disease.D_ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Part.Part_ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Symptom.S_ID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Disease.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Part.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Symptom.Description)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.DS_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.DS_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.DS_ID })
        </td>
    </tr>
}

</table>
